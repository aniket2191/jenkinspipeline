
pipeline{

	agent any
	
	stages
	{
		stage("Build")
		{
			steps
			{
			echo "Building the Project...."	
			}
		}
	
		stage("Test")
		{
			steps
			{
			echo "Testinging the Project...."	
			}
		}

		stage("Deploy")
		{
			step
			{
			echo "Deploying the Project...."	
			}
			
		}

		stage("Release")
		{
			step
			{
			echo "Releasing the Project...."
			}
		
		}

	}	
}
